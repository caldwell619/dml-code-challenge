{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "standard"
  ],
  "rules": {
    // "@typescript-eslint/no-explicit-any": 2,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "none",
        "tabWidth": 2,
        "printWidth": 120,
        "singleQuote": true,
        "semi": false,
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "jsxSingleQuote": true
      }
    ]
  }
}